## Chdir to directory of current buffer
function cde() {
    local EMACS_CWD=$(emacsclient -e \
      '(editutil-current-buffer-directory)' | sed 's/^"\(.*\)"$/\1/' )

    if [ "$EMACS_CWD" == "nil" ]; then
        echo "Failed cde"
        return 1
    fi

    echo "chdir to $EMACS_CWD"
    cd "$EMACS_CWD"
}
